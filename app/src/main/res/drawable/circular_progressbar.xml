<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="ring" >

    <solid android:color="#e1e1e1" />

    <stroke
        android:width="2dp"
        android:color="@color/DarkBlue" />


    <corners android:radius="50dp" />

    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />

</shape>